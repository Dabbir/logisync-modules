<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="logistics_view_partner_list" model="ir.ui.view">
    <field name="name">logistics.partner.list</field>
    <field name="model">logistics.partner</field>
    <field name="arch" type="xml">
      <list string="Mitra Logistik">
        <field name="name"/>
        <field name="contact_info"/>
      </list>
    </field>
  </record>

  <record id="logistics_view_partner_form" model="ir.ui.view">
    <field name="name">logistics.partner.form</field>
    <field name="model">logistics.partner</field>
    <field name="arch" type="xml">
      <form string="Mitra Logistik">
        <group>
          <field name="name" />
          <field name="contact_info" />
        </group>
      </form>
    </field>
  </record>

  <record id="logistics_action_partner" model="ir.actions.act_window">
    <field name="name">Mitra Logistik</field>
    <field name="res_model">logistics.partner</field>
    <field name="view_mode">list,form</field>
  </record>
</odoo>